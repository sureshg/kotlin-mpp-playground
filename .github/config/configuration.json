{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": [
        "feat"
      ]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": [
        "fix"
      ]
    },
    {
      "title": "## 🧰 Maintenance",
      "labels": [
        "chore"
      ]
    },
    {
      "title": "## 🧪 Tests",
      "labels": [
        "test"
      ]
    },
    {
      "title": "## 🖍️ Documentation",
      "labels": [
        "doc",
        "docs"
      ]
    },
    {
      "title": "## 📦 Dependencies",
      "labels": [
        "deps",
        "dependencies",
        "chore(deps)",
        "build(deps)"
      ]
    },
    {
      "title": "## 💬 Other",
      "labels": [
        "other",
        "misc"
      ]
    }
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
  "pr_template": "${{TITLE}}",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.+): (.+)",
      "target": "$1"
    }
  ],
  "exclude_merge_branches": [
    "merge pull request",
    "Merge pull request"
  ]
}
